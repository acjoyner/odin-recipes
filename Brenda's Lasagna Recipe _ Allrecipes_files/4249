    function isIEBrowserVersion9_742057() {
        return (isIEBrowser_742057 && (browserVersion_742057 == 9)) ? true : false;
    }

    function isEdgeSupported_742057() {
        if(isIEBrowserVersion9_742057()) {
            return "y";           //hardcoding IE9 edge support.
        }
        if(hasTransform_742057) {
            if(requiresSVG_742057&&!hasSVG_742057)
                return "f";
            return "y";
        }
        return "f";
    }

    function isCanvasSupported_742057(){
      var elem = document.createElement('canvas');
      return !!(elem.getContext && elem.getContext('2d'));
    }

    function isHTML5FeaturesSupported_742057() {
         return (isCanvasSupported_742057()) ? "y" : "f";
    }

    var requiresSVG_742057=false;
