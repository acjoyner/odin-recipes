     function getInternetExplorerVersion_742057() {
         // Returns the version of Internet Explorer or a -1
         // (indicating the use of another browser).

             var rv = -1; // Return value assumes failure.
             if (navigator.appName == 'Microsoft Internet Explorer') {
                 isIEBrowser_742057=true;
                 var ua = navigator.userAgent;
                 var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");

                 if (re.exec(ua) != null)
                     rv = parseFloat( RegExp.$1 );
             }

             return rv;
         }

      //returns true if ie version is less than 9, say ie6, ie7, ie8
         // -1 for non IE browsers.
         function isIEBrowserWithVersionLessThan9_742057 () {

             browserVersion_742057 = getInternetExplorerVersion_742057();  //-1 for non IE browsers
             if((browserVersion_742057 != -1) && (browserVersion_742057 < 9)) {
                 return true;

             }
             return false;
         }

    //code to detect Edge Features, courtesy  (http://dl.dropboxusercontent.com/u/13483458/test-edge.html)
    var testEle_742057=document.createElement("div_742057");
    function isSupported_742057(a){

        var d=testEle_742057.style,e;
        for(i=0;i<a.length;i++)
            if(e=a[i],d[e]!==void 0)
                return!0;
        return!1
    }

    function supportsRGBA_742057(){

        testEle_742057.cssText="background-color:rgba(150,255,150,.5)";
        if((""+testEle_742057.style.backgroundColor).indexOf("rgba")==0)
            return!0;
        return!1
    }

    var hasTransform_742057=isSupported_742057([
        "transformProperty",
